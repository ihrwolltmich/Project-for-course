obesityrateandHappinessEasternEurope=pd.merge(obesityrateinCentralandEasternEurope,CentralandEasternEurope)
print(obesityrateandHappinessEasternEurope.to_string())
fig,ax=plt.subplots()
ax.scatter(obesityrateandHappinessEasternEurope['Both.sexes'],obesityrateandHappinessEasternEurope['Happiness Score'],color='blue',label='Countires in Eastern and Central Europe')
ax.legend()
plt.title("Comparing Countries in Central and Eastern Europe")
ax.set_xlabel('Obesity rate in Percentage')
ax.set_ylabel('Happiness Score')
plt.show()
